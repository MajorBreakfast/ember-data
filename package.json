{
  "name": "components-ember-data",
  "version": "1.0.0-beta.9",
  "description": "A data persistence library for Ember.js.",
  "keywords": ["ember"],
  "main": "./ember-data.js",
  "dependencies": {
    "ember": "> 1.0.0"
  },
  "jspm": {
    "main": "ember-data",
    "shim": {
      "ember-data": { "deps": ["ember"], "exports": "DS" },
      "ember-data.prod": { "deps": ["ember"], "exports": "DS" }
    },
    "registry": "jspm",
    "dependencies": {
      "ember": "github:MajorBreakfast/ember@master"
    },
    "files": ["ember-data.js", "ember-data.prod.js"],
    "buildConfig": { "minify": true }
  }
}
